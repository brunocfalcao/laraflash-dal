{
    "name": "brunocfalcao/laraflash-dal",
    "description": "Laraflash Data Access Layer",
    "type": "library",
    "keywords": [
        "laraflash",
        "laravel",
        "brunocfalcao"
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^6.0",
        "brunocfalcao/feeds": "^1.2",
        "brunocfalcao/helpers": "^1.0",
        "kitetail/zttp": "^0.5.0",
        "genealabs/laravel-model-caching": "*",
        "intervention/image": "^2.5",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Laraflash\\DAL\\": "src/"
        },
        "files": [
            "src/helpers.php",
            "src/simpledom.php"
        ]
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno Falcao",
            "email": "bruno.c.falcao@brunofalcao.me"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Laraflash\\DAL\\LaraflashDALServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
